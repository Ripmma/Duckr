{
	parser: "babel-eslint",
	env: {
		es6: ture,
		browser: true
	},
	parserOptions: {
		ecmaversion: 6,
		sourceType: "module",
		ecmaFeatures: {
			jsx: true,
			experimentalObjectRestSpread: true
		}
	},
	extends: ["eslint:recommended", "plugin:react/recommended", "standard"],
	plugins: [
		"react",
	],
	"rules": {
		"no-console": [2, {allow: ["warn", "error"]}],
		"comma-dangle": [2, "always-multiline"],
		"semi": [2, "never"]
	}
}